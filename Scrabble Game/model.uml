                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_1lffMF_lEeSVu5N5I_41GQ" name="takeAction" specification="_1j48sl_lEeSVu5N5I_41GQ">
                    <ownedParameter xmi:id="_1lffMV_lEeSVu5N5I_41GQ" name="game" type="_1fzt81_lEeSVu5N5I_41GQ"/>
                    <language>java</language>
                    <body>Board board = game.getBoard();

		
		int x = loc.getX();
		int y = loc.getY();
		int score = 0;
		
		for(int i = -3; i&lt; 4; i++){
			if((x+i) >= 0)
				score += board.bombSquares( x+i, y);
		}
		
		for(int i = -3; i&lt; 4; i++){
			if((y+i) >= 0)
				score += board.bombSquares(x, y + i);
		}
		
		game.getCurrentPlayer().deductScore(score);
		System.out.println(&quot;bomb : &quot; + score);
		
		
	
		
		
	</body>
