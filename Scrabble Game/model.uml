                    </ownedParameter>
                    <ownedParameter xmi:id="_1ls6k1_lEeSVu5N5I_41GQ" name="property" type="_1fyf0F_lEeSVu5N5I_41GQ"/>
                    <language>java</language>
                    <body>squares[x][y] = new Square(property);
		squares[14 - x][14 - y] = new Square(property);
		squares[14 - x][y] = new Square(property);
		squares[x][14 - y] = new Square(property);

	</body>
