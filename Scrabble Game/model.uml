                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_1luItF_lEeSVu5N5I_41GQ" name="getWordTiles" specification="_1kqYwF_lEeSVu5N5I_41GQ">
                    <ownedParameter xmi:id="_1luItV_lEeSVu5N5I_41GQ" name="word" type="_1gitwF_lEeSVu5N5I_41GQ"/>
                    <ownedParameter xmi:id="_1luItl_lEeSVu5N5I_41GQ" name="map" type="_1kARcF_lEeSVu5N5I_41GQ"/>
                    <ownedParameter xmi:id="_1luIt1_lEeSVu5N5I_41GQ" type="_1j0EMF_lEeSVu5N5I_41GQ" direction="return"/>
                    <language>java</language>
                    <body>LinkedList&lt;Tile> wordTile = new LinkedList&lt;Tile>();
		Tile tempTile;
		Location tempLoc;
		int startX, startY, endX, endY;
		startX = word.getStartX();
		startY = word.getStartY();
		endX = word.getEndX();
		endY = word.getEndY();
		// for row word
		if (startX == endX) {
			for (int i = startY; i &lt;= endY; i++) {
				tempTile = squares[startX][i].getTile();
				if (tempTile == null) {
					Iterator&lt;Map.Entry&lt;Location, Tile>> entries = map
							.entrySet().iterator();
					while (entries.hasNext()) {
						Map.Entry&lt;Location, Tile> entry = entries.next();
						tempLoc = entry.getKey();
						if (tempLoc.getX() == startX &amp;&amp; tempLoc.getY() == i) {
							tempTile = entry.getValue();
						}
					}
				}
				wordTile.add(tempTile);
			}
	
		}
		// for col word 
		else if (startY == endY) {
			for (int i = startX; i &lt;= endX; i++) {
				tempTile = squares[i][startY].getTile();
				if (tempTile == null) {
					Iterator&lt;Map.Entry&lt;Location, Tile>> entries = map
							.entrySet().iterator();
					while (entries.hasNext()) {
						Map.Entry&lt;Location, Tile> entry = entries.next();
						tempLoc = entry.getKey();
						if (tempLoc.getX() == i &amp;&amp; tempLoc.getY() == startY) {
							tempTile = entry.getValue();
						}
					}
				}
				wordTile.add(tempTile);
			}
	
		}
		word.setTiles(wordTile);
		return wordTile;
	
	</body>
                  </ownedBehavior>
                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_1luIuF_lEeSVu5N5I_41GQ" name="getWordString" specification="_1krm4V_lEeSVu5N5I_41GQ">
