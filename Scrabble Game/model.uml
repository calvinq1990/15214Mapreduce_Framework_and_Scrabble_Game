                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_1leRE1_lEeSVu5N5I_41GQ" name="addSpecialTile" specification="_1e5H8F_lEeSVu5N5I_41GQ">
                    <ownedParameter xmi:id="_1leRFF_lEeSVu5N5I_41GQ" name="index">
                      <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
                    </ownedParameter>
                    <ownedParameter xmi:id="_1leRFV_lEeSVu5N5I_41GQ" direction="return">
                      <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
                    </ownedParameter>
                    <language>java</language>
                    <body>SpecialTile specialTile = null;

		switch (index) {
		case 0:
			specialTile = new BoomTile();
			break;
		case 1:
			specialTile = new NegativeTile();
			break;
		case 2:
			specialTile = new ReverseTile();
			break;
		case 3:
			specialTile = new BoomTile();
			break;

		}
		for (int i = 0; i &lt; 7; i++) {
			if (specialTiles[i] == null) {
				specialTiles[i] = specialTile;
				return i;
			}

		}
		return -2;

	</body>
                  </ownedBehavior>
